    "babel-loader": "8.1.0"
  },
  "scripts": {
    "start": "PORT=4000 react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "storybook": "NODE_PATH=src start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "info": "serverless info --aws-profile $npm_config_profile --stage $npm_config_stage --region us-west-2 -v",
    "predeploy": "npm run build",
    "deploy": "aws s3 sync build/ s3://$npm_config_websitebucket --delete --profile $npm_config_profile --region us-west-2",
    "postdeploy": "aws cloudfront create-invalidation --profile $npm_config_profile --region us-west-2 --distribution-id $npm_config_distribution --paths '/*'",
    "lint": "eslint ./src --ext ts,tsx",
    "lint:fix": "eslint --fix . --ext ts,tsx",
    "format": "prettier --write \"**/*.+(ts|tsx|json|css|md)\"",
    "generate:exports": "node ./bin/generateExports"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier/recommended"
    ]
  },
  "jest": {
    "verbose": true,
    "moduleNameWrapper": {
      "^~(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!react-children-utilities).+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ]
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "lint-staged": {
    "*.+(ts|tsx)": "eslint --fix",
    "*.+(json|css|md)": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
